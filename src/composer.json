{
    "name": "market/test-code-review",
    "type": "project",
    "require": {
        "slim/slim": "4.*",
        "phpunit/phpunit": "4.1.*",
        "slim/psr7": "^1.5",
        "vlucas/valitron": "^1.4",
        "ext-json": "*",
        "php-di/slim-bridge": "3.2.*",
        "league/pipeline": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "*"
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml --do-not-cache-result --colors=always"
    }
}
